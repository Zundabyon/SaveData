<div id="<%= dom_id user %>">
  <p>
    <strong>メールアドレス:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>名前:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>生年月日:</strong>
    <%= user.birthday %>
  </p>

  <p>
    <strong>性別:</strong>
    <%= user.gender %>
  </p>

  <p>
    <strong>パスワード:</strong>
    <%= user.crypted_password %>
  </p>

</div>
